New-Item -ItemType Directory -Path "C:\Datei\Buchhaltung"
New-Item -ItemType Directory -Path "C:\Datei\Verwaltung"

Set-Location "C:\Datei\Buchhaltung"


Set-ExecutionPolicy Unrestricted

